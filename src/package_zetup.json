{
  "package_add": "package_add.json",
  "package_update": "package_update.json",
  "projects": [
    "lamed_core",
    "lamed_test",
    "lamed_string",
    "lamed_io",
    "lamed_folder",
    "lamed_vscode",
    "lamed_markdown",
    "lamed_name",
    "lamed_flowchart",
    "lamed_floweditor",
    "lamed_learn",
    "see_it_done",
    "see_it_start"
  ],
  "projects_add_exclude": [
    "lamed_test",
    "lamed_core",
    "lamed_folder"
  ],
  "projects_gitignore_exclude": [

  ],
  "projects_dependencies_exclude": [
    "babel-eslint",
    "mocha",
    "snazzy",
    "standard",
    "codecov",
    "nyc"
  ],
  "commands" : {
    "pull" : {"npm" : "start npm run pull", "timeout" : 10, "description" : "Pull changes from master for all projects and do yarn"},
    "commitSafe" : {"npm" : "start npm run commitAll", "description" : "Run tests and if success commit changes to master"},
    "commitFast" : {"npm" : "start npm run commitFast", "timeout" : 15, "description" : "Fast commit to master (no tests)"},
    "commitLocal" : {"npm" : "start npm run commit", "timeout" : 0, "description" : "Local commit only"},
    "upgrade" : {"npm" : "start npm run upgrade", "timeout": 55, "description": "Upgrade all project dependencies; on change inc version and release to master"},
    "release": {"npm": "start npm run release", "timeout": 10, "description" : "Release committed changes to master (no version inc)"},
    "test" : {"npm": "start npm test", "timeout" : 3, "description": "Test all projects"},
    "testYarn" : {"npm": "start npm run test_yarn", "timeout" : 7, "description": "Do yarn and test all modules"},
    "gitfix" : {"npm": "start npm run gitfix", "timeout": 3, "description": "Ensure .gitIgnore files are ignored for all projects"},
    "setup" : {"cmd":  "git clone https://github.com/perezLamed/$repo_name$.git", "description" :  "Setup the lamed repos"},
    "setupYarn" : {"npm": "start npm run test_yarn", "timeout" : 55, "description": "Do yarn and test all new modules"}
  },
  "timeout" : "30",
  "travis" : {
    "os" : ["linux", "osx"],
    "os_quick": ["linux"],
    "node_js": ["node", "'11'", "'10'", "'8'", "'6'", "lts/*"],
    "node_js_quick": ["lts/*"]
  },
  "travis_exclude" : [
    "lamed_vscode"
  ],
  "travis_mode": "quick"
}