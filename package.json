{
  "name": "see_it_done",
  "version": "0.0.1",
  "description": "see_it_done ??????",
  "main": "src/index.js",
  "url": "https://www.npmjs.com/package/see_it_done",
  "scripts": {
    "test": "mocha \"./{,!(node_modules)/**/}*.runner.js\"",
    "test2": "nodemon --exec \"npm test\"",
    "coverage_test": "nyc mocha \"./{,!(node_modules)/**/}*.runner.js\"",
    "coverage_report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage": "npm run coverage_test && npm run coverage_report",
    "rules": "standard | snazzy",
    "fix": "standard --fix",
    "update": "ncu -u && yarn",
    "commit": "git commit -a -m \"release\"",
    "bump": "npm version patch -m \"Bumping to %s\"",
    "push": "echo Push and release patch to master... && echo ------------------------------ && git push origin master",
    "release": "npm run rules && npm test && npm version patch -m \"Patch version %s\" && start npm run push"
  },
  "nyc": {
    "exclude": [
      "**/Test.runner.js",
      "**/readline.js**"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "mocha"
    ],
    "ignore": [
      "**/out/",
      "/lib/select2/",
      "/lib/ckeditor/",
      "tmp.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perezLamed/see_it_done.git"
  },
  "author": "Perez Lamed van Niekerk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perezLamed/see_it_done/issues"
  },
  "homepage": "https://github.com/perezLamed/see_it_done#readme",
  "dependencies": {
    "lamed_array": "^1.0.0",
    "lamed_io": "^1.0.9",
    "lamed_test": "^2.1.15"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "codecov": "^3.2.0",
    "expect": "^24.1.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
